{
  "manifest_version": 3,
  "name": "PR Test File Identifier",
  "version": "1.0",
  "description": "Visually identifies test files in GitHub PRs",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ]
}
